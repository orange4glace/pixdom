{
  "name": "PixDOM",
  "version": "1.0",
  "description": "Pixel measurement extension",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}